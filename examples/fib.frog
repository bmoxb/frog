# Calculate the Fibonacci number at the given index.
let fib n : int -> int =
    let loop a b n : int int int -> int =
        if n > 0 then loop b (a + b) (n - 1)
        else a
    in
    loop 0 1 n

let main : @in -> @out =
    @out "n = ";
    let input : int = @in in
    @out "F_n = " (fib input)
