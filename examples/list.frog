data list = Cons int list | Nil

let is_second_element n head : int list -> bool =
    match head with
    | Cons n (Cons m tail) -> m == head
    | Cons n (Nil)           -> False
    | Nil                    -> False

let main : @in -> @out =
    let input : int = @in in
    let l : list = Cons 1 (Cons 2 (Cons 3 Nil)) in
    @out (is_second_element input l)
